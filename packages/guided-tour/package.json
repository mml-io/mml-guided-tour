{
  "name": "@mml-guided-tour/mml-doc",
  "private": true,
  "scripts": {
    "build": "tsx ./build.ts --build --local",
    "dist": "dotenv -e .env.local -- tsx ./build.ts --dist",
    "upload": "dotenv -e .env.local -- mml-uploader --build-dir ./dist",
    "iterate": "tsx ./build.ts --local --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c ./.eslintrc.cjs \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint -c ./.eslintrc.cjs \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "@private/uploader": "0.0.1",
    "canvas": "^2.11.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@mml-io/esbuild-plugin-mml": "0.3.0",
    "@mml-io/mml-react-types": "0.19.3",
    "@types/node": "22.13.14",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "^10.0.0",
    "dotenv-cli": "^7.4.2",
    "form-data": "^4.0.3",
    "msw": "^2.10.2",
    "node-fetch": "^3.3.2",
    "tsx": "^4.19.1",
    "uuid": "^10.0.0"
  }
}