{
  "name": "@mml-guided-tour/mml-doc",
  "private": true,
  "scripts": {
    "build": "MSERVE_PROJECT=guided-tour-9709e5 tsx ./build.ts --build",
    "deploy": "run-p deploy:*",
    "deploy:mml": "mserve deploy --project guided-tour-9709e5 build",
    "deploy:assets": "gcloud storage rsync build/assets gs://mml-guided-tour-assets/assets --recursive --delete-unmatched-destination-objects",
    "iterate": "tsx ./build.ts --build --local --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c ./.eslintrc.cjs \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint -c ./.eslintrc.cjs \"./src/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@mml-io/esbuild-plugin-mml": "^0.2.1",
    "@mml-io/mml-react-types": "0.18.0",
    "@mserve-io/mserve": "^0.3.1",
    "@types/proper-lockfile": "^4.1.4",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@types/uuid": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "proper-lockfile": "^4.1.2",
    "tsx": "^4.19.1",
    "uuid": "^10.0.0"
  }
}
